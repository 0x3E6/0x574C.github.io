    <id>http://longwang.live/2020/02/21/%E4%B8%80%E7%A7%8D%E5%A4%84%E7%90%86Sqoop%E5%AF%BC%E5%87%BA%E8%BF%87%E7%A8%8B%E4%B8%AD%E6%95%B0%E6%8D%AE%E7%9A%84%E6%96%B9%E6%B3%95/</id>
